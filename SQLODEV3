SELECT country FROM country WHERE country LIKE 'A%a' ;
SELECT * FROM country WHERE country LIKE '%_____n' ; 
SELECT title FROM film WHERE title ILIKE '%t%' LIMIT 4 ; --  SELECT title FROM film WHERE title ILIKE '%T%T%T%T%'
SELECT * FROM film WHERE length(title) > 90 AND title LIKE 'C%' and rental_rate = 2.99 ;
